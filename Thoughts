// <Category name= {this.state.response[0]["categories"].find(item => item.name = "Beef")} />



getCategory(category)
  this.state.response.find(item => item.name = category)
render() {
  return(
    <div>
    {this.getCategory("Pre Burger")}
    </div>
  )
  }
}
